#include <stdio.h>

int main(){
    int angka[3];
    int i;
    int biggest;

    for (i=0; i<3;i++){
        scanf("%d", &angka[i]);
    }

    biggest = angka[0];
    for (i=1;i<3;i++){
        if (biggest < angka[i]){
            biggest = angka[i];
        }
    }

    // output
    printf("Diantara %d, %d, dan %d. Angka terbesar adalah %d\n", angka[0], angka[1], angka[2], biggest);

    return 0;
}
